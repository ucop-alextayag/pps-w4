          SKIP1                                                         PPP410  
       SQL-UPDATE-SCR-9540 SECTION.                                     PPP410  
           PERFORM SCR-DATE-CONVERSION-9550.                            PPP410  
           MOVE 'UPDATE SCR ROW' TO DB2MSG-TAG.                         PPP410  
           EXEC SQL                                                     PPP410  
             UPDATE PPPVZSCR_SCR                                        28521087
             SET SCR_NEXTCYCLE_MO                                       PPP410  
             =  :SCR-NEXTCYCLE-MO  ,                                    PPP410  
                 SCR_NEXTCYCLE_QTR                                      PPP410  
             =  :SCR-NEXTCYCLE-QTR ,                                    PPP410  
                 SCR_NEXTCYCLE_YR                                       PPP410  
             =  :SCR-NEXTCYCLE-YR  ,                                    PPP410  
                 SCR_NEXTCYCLE_FYR                                      PPP410  
             =  :SCR-NEXTCYCLE-FYR ,                                    PPP410  
                 SCR_LAST_PGM_RUN                                       PPP410  
             =  :SCR-LAST-PGM-RUN  ,                                    PPP410  
                 SCR_CYCLES_PAID1                                       PPP410  
             =  :SCR-CYCLES-PAID1  ,                                    PPP410  
                 SCR_CYCLES_PAID2                                       PPP410  
             =  :SCR-CYCLES-PAID2  ,                                    PPP410  
                 SCR_CYCLES_PAID3                                       PPP410  
             =  :SCR-CYCLES-PAID3  ,                                    PPP410  
                 SCR_CYCLES_PAID4                                       PPP410  
             =  :SCR-CYCLES-PAID4                                       PPP410  
             WHERE SCR_KEY = :SCR-KEY                                   PPP410  
           END-EXEC.                                                    PPP410  
       END-SQL-UPDATE-SCR-9540.                                         PPP410  
           EXIT.                                                        PPP410  
