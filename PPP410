                                                                        69161571
           MOVE 'OPEN DBL CURSOR' TO DB2MSG-TAG.                        69161571
           EXEC SQL                                                     69161571
               OPEN DBL_ROW                                             69161571
           END-EXEC.                                                    69161571
           MOVE 'FETCH DBL ROW' TO DB2MSG-TAG.                          69161571
           EXEC SQL                                                     69161571
               FETCH DBL_ROW                                            69161571
                 INTO                                                   69161571
                  :DBL-ROW-DATA.EMPLOYEE-ID                             69161571
                 ,:DBL-ROW-DATA.GTN-NUMBER                              69161571
                 ,:GTN-IND                                              69161571
                 ,:GTN-AMT                                              69161571
           END-EXEC.                                                    69161571
           PERFORM                                                      69161571
             UNTIL SQLCODE = +100                                       69161571
               OR BAL-IDX = IDC-MAX-CPA-PAR-BALS                        69161571
               ADD 1 TO BAL-IDX                                         69161571
               MOVE GTN-NUMBER OF DBL-ROW-DATA TO WS-BAL-GTN            69161571
               MOVE WS-BAL-NUMBER TO XPAR-BALS-NUMBER (BAL-IDX)         69161571
               MOVE GTN-IND TO XPAR-BALS-IND (BAL-IDX)                  69161571
               MOVE GTN-AMT TO XPAR-BALS-AMT (BAL-IDX)                  69161571
               EXEC SQL                                                 69161571
                   FETCH DBL_ROW                                        69161571
                     INTO                                               69161571
                      :DBL-ROW-DATA.EMPLOYEE-ID                         69161571
                     ,:DBL-ROW-DATA.GTN-NUMBER                          69161571
                     ,:GTN-IND                                          69161571
                     ,:GTN-AMT                                          69161571
               END-EXEC                                                 69161571
           END-PERFORM.                                                 69161571
           MOVE 'CLOSE DBL CURSOR' TO DB2MSG-TAG.                       69161571
           EXEC SQL                                                     69161571
               CLOSE DBL_ROW                                            69161571
           END-EXEC.                                                    69161571
                                                                        69161571
           MOVE BAL-IDX TO XPAR-NO-BALS.                                69161571
