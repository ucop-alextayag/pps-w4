               MOVE 5558 TO XPAR-BALS-NUMBER (BAL-IDX)                  17121738
               MOVE ' ' TO XPAR-BALS-IND (BAL-IDX)                      17121738
               MOVE YTD-NQPLNS-W2BOX11   OF PCM-ROW-DATA                17121738
                 TO XPAR-BALS-AMT (BAL-IDX)                             17121738
           END-IF.                                                      17121738
           IF YTD-409A-DEFERRALS  OF PCM-ROW-DATA NOT = ZERO            25151874
               ADD 1 TO BAL-IDX                                         25151874
               MOVE 5559 TO XPAR-BALS-NUMBER (BAL-IDX)                  25151874
               MOVE ' ' TO XPAR-BALS-IND (BAL-IDX)                      25151874
               MOVE YTD-409A-DEFERRALS   OF PCM-ROW-DATA                25151874
                 TO XPAR-BALS-AMT (BAL-IDX)                             25151874
           END-IF.                                                      25151874
           IF YTD-409A-INCOME     OF PCM-ROW-DATA NOT = ZERO            25151874
               ADD 1 TO BAL-IDX                                         25151874
               MOVE 5560 TO XPAR-BALS-NUMBER (BAL-IDX)                  25151874
               MOVE ' ' TO XPAR-BALS-IND (BAL-IDX)                      25151874
               MOVE YTD-409A-INCOME      OF PCM-ROW-DATA                25151874
                 TO XPAR-BALS-AMT (BAL-IDX)                             25151874
           END-IF.                                                      25151874
           IF YTD-HCRA-UNS-CLAIM  OF PCM-ROW-DATA NOT = ZERO            25151874
               ADD 1 TO BAL-IDX                                         25151874
               MOVE 5561 TO XPAR-BALS-NUMBER (BAL-IDX)                  25151874
               MOVE ' ' TO XPAR-BALS-IND (BAL-IDX)                      25151874
