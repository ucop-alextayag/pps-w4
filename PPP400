       9902-FETCH-BUF  SECTION.                                         71921294
      *----------------------------------------------------------------*71921294
           EXEC SQL                                                     71921294
                FETCH BUF_ROW                                           71921294
                 INTO  :BUF-BUC,                                        71921294
                       :BUF-REP,                                        71921294
                       :BUF-SHC,                                        71921294
                       :BUF-DISTRIBUTION,                               71921294
                       :BUF-UDUE-GTN,                                   71921294
                       :BUF-AGENCY-FEE-GTN,                             71921294
                       :BUF-AGENCY-FEE-VAL,                             71921294
                       :BUF-CHARITY-GTN1,                               71921294
                       :BUF-CHARITY-GTN2,                               71921294
                       :BUF-CHARITY-GTN3,                               71921294
                       :BUF-UNIT-LINK,                                  71921294
                       :BUF-UDUE-BASE,                                  71921294
                       :BUF-AF-BASE                                     71921294
           END-EXEC.                                                    71921294
           IF SQLCODE = +100                                            71921294
             SET NO-MORE-RECS TO TRUE                                   71921294
           ELSE                                                         71921294
             ADD 1 TO BUFT-LOADED                                       71921294
             IF   BUFT-LOADED > BUFT-MAX                                71921294
      *--------> PPPBUF TABLE OVERFLOW                                  71921294
               MOVE M40232 TO XMSG-NUMBER                               71921294
               MOVE ZEROS TO XMSG-ID-NO                                 71921294
               PERFORM CALL-MESSAGE-HANDLER-6080                        71921294
               SET NO-MORE-RECS TO TRUE                                 71921294
             ELSE                                                       71921294
               MOVE BUF-BUC          TO BUFT-KEY-BUC      (BUFT-LOADED) 71921294
               MOVE BUF-REP          TO BUFT-KEY-REP      (BUFT-LOADED) 71921294
               MOVE BUF-SHC          TO BUFT-KEY-SHC      (BUFT-LOADED) 71921294
               MOVE BUF-DISTRIBUTION TO BUFT-KEY-DUC      (BUFT-LOADED) 71921294
               MOVE BUF-UDUE-GTN     TO BUFT-UDUE-GTN     (BUFT-LOADED) 71921294
               MOVE BUF-AGENCY-FEE-GTN                                  71921294
                                  TO BUFT-AGENCY-FEE-GTN (BUFT-LOADED)  71921294
               MOVE BUF-AGENCY-FEE-VAL                                  71921294
                                  TO BUFT-AGENCY-FEE-VAL  (BUFT-LOADED) 71921294
               MOVE BUF-CHARITY-GTN1 TO BUFT-CHARITY-GTN1 (BUFT-LOADED) 71921294
               MOVE BUF-CHARITY-GTN2 TO BUFT-CHARITY-GTN2 (BUFT-LOADED) 71921294
               MOVE BUF-CHARITY-GTN3 TO BUFT-CHARITY-GTN3 (BUFT-LOADED) 71921294
               MOVE BUF-UDUE-BASE    TO BUFT-UDUE-BASE    (BUFT-LOADED) 71921294
               MOVE BUF-AF-BASE      TO BUFT-AF-BASE      (BUFT-LOADED) 71921294
               MOVE BUF-UNIT-LINK    TO BUFT-UNIT-LINK    (BUFT-LOADED) 71921294
               IF   BUF-UNIT-LINK  NOT = SPACE                          71921294
                  MOVE BUF-UNIT-LINK    TO BUFT-88-FULLY-LINKED-TEST    71921294
                  IF BUFT-88-FULLY-LINKED-UNIT                          71921294
                     MOVE SPACE     TO BUFT-UNIT-LINK    (BUFT-LOADED)  71921294
                  ELSE                                                  71921294
                     CONTINUE                                           71921294
                  END-IF                                                71921294
               END-IF                                                   71921294
      *----------> THE UDUE GTN, AGENCY FEE GTN, AND CHARITY #1 GTN     71921294
      *----------> ON THE PPPBUF TABLE MUST BE NON-ZERO                 71921294
               IF   BUFT-UDUE-GTN     (BUFT-LOADED)  > ZERO             71921294
                  CONTINUE                                              71921294
               ELSE                                                     71921294
                  MOVE M40233 TO XMSG-NUMBER                            71921294
                  MOVE ZEROS TO XMSG-ID-NO                              71921294
                  PERFORM CALL-MESSAGE-HANDLER-6080                     71921294
                  SET NO-MORE-RECS TO TRUE                              71921294
               END-IF                                                   71921294
             END-IF                                                     71921294
           END-IF.                                                      71921294
                                                                        71921294
